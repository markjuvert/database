#
# FREE format for table translation
#
TABLE=translation
   DELIMITER=,
   SPACE_FILL=Y
   QUALIFIER=NONE
      FIELD=table_name
         COLUMN=1
      FIELD=field_name
         COLUMN=2
      FIELD=code
         COLUMN=3
      FIELD=num_equiv
         COLUMN=4
      FIELD=description
         COLUMN=5
